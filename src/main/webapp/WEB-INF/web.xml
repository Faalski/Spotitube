<!DOCTYPE web-app PUBLIC
        "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
        "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app
        xmlns="http://xmlns.jcp.org/xml/ns/javaee"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee
    http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
        metadata-complete="false"
        version="3.1"
>
    <display-name>Archetype Created Web Application</display-name>



    <servlet>
        <servlet-name>ViewPlaylist</servlet-name>
        <servlet-class>Presentation.ViewPlaylistsController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ViewTracks</servlet-name>
        <servlet-class>Presentation.ViewTracksController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Login</servlet-name>
        <servlet-class>Presentation.LoginController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ChangePlaylistName</servlet-name>
        <servlet-class>Presentation.ChangePlaylistNameController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CreatePlaylist</servlet-name>
        <servlet-class>Presentation.CreatePlaylistController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ViewTracksFromPlaylist</servlet-name>
        <servlet-class>Presentation.ViewTracksFromPlaylistController</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>ViewTracks</servlet-name>
        <url-pattern>/ViewTracks</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ViewPlaylist</servlet-name>
        <url-pattern>/ViewPlaylist</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Login</servlet-name>
        <url-pattern>/Login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ChangePlaylistName</servlet-name>
        <url-pattern>/ChangePlaylistName</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CreatePlaylist</servlet-name>
        <url-pattern>/CreatePlaylist</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ViewTracksFromPlaylist</servlet-name>
        <url-pattern>/ViewTracksFromPlaylist</url-pattern>
    </servlet-mapping>

    <welcome-file-list>
        <welcome-file>start.jsp</welcome-file>
    </welcome-file-list>



</web-app>
